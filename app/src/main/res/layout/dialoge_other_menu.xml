<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_gravity="center"
	android:gravity="center_horizontal"
	android:layout_height="match_parent">



	<androidx.cardview.widget.CardView
		android:layout_width="@dimen/_250sdp"
		android:layout_height="wrap_content"
		app:cardCornerRadius="@dimen/_10sdp"
		android:elevation="@dimen/_10sdp"
		app:cardUseCompatPadding="true">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="@color/white"
			android:orientation="vertical">



			<LinearLayout
				android:id="@+id/dialog_other_menu_lv_Ads_list"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="@dimen/_10sdp">

				<ImageView
					android:layout_width="@dimen/_20sdp"
					android:layout_height="@dimen/_20sdp"
					android:src="@drawable/ic_add_list"/>

				<TextView

					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_marginLeft="@dimen/_10sdp"
					android:gravity="center_vertical"
					android:text="Ads List"
					android:textColor="@color/TextColor"
					android:textSize="@dimen/_12sdp" />

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="@dimen/_1sdp"
				android:background="@color/grey"/>


			<LinearLayout
				android:id="@+id/dialog_other_menu_lv_my_account"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="@dimen/_10sdp">

				<ImageView
					android:layout_width="@dimen/_20sdp"
					android:layout_height="@dimen/_20sdp"
					android:src="@drawable/ic_update"/>

				<TextView

					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:gravity="center_vertical"
					android:layout_marginLeft="@dimen/_10sdp"
					android:textColor="@color/TextColor"
					android:textSize="@dimen/_12sdp"
					android:text="Setting"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="@dimen/_1sdp"
				android:background="@color/grey"/>

			<LinearLayout
				android:id="@+id/dialog_other_menu_lv_update_profile"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="@dimen/_10sdp">

				<ImageView
					android:layout_width="@dimen/_20sdp"
					android:layout_height="@dimen/_20sdp"
					android:src="@drawable/ic_update_profile"/>

				<TextView

					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:gravity="center_vertical"
					android:layout_marginLeft="@dimen/_10sdp"
					android:textColor="@color/TextColor"
					android:textSize="@dimen/_12sdp"
					android:text="Update Profile"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="@dimen/_1sdp"
				android:background="@color/grey"/>



			<LinearLayout
				android:id="@+id/dialog_other_menu_lv_logout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="@dimen/_10sdp">

				<ImageView
					android:layout_width="@dimen/_20sdp"
					android:layout_height="@dimen/_20sdp"
					android:src="@drawable/ic_logout"/>

				<TextView

					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:gravity="center_vertical"
					android:layout_marginLeft="@dimen/_10sdp"
					android:textColor="@color/TextColor"
					android:textSize="@dimen/_12sdp"
					android:text="Logout"/>

			</LinearLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="@dimen/_1sdp"
				android:background="@color/grey"/>



		</LinearLayout>
	</androidx.cardview.widget.CardView>

</LinearLayout>